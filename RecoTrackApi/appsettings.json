{
  "MongoDbSettings": {
    "DatabaseName": "StudentRoutineTrackerDB",
    "LogsCollectionName": "logs",
    "EmailAuditCollectionName": "email_audit"
  },
  "JwtSettings": {
    "Issuer": "RecoTrackAPI - Service",
    "Audience": "RecoTrackWeb - Client",
    "ExpiryMinutes": 60
  },
  "AllowedHosts": "*",
  "FrontendURL": "https://recotrackpiyushsingh.vercel.app",
  "OpenRouter": {
    "ApiUrl": "https://openrouter.ai/api/v1/chat/completions"
  },
  "GitHub": {
    "ApiUrl": "https://api.github.com"
  },
  "Smtp": {
    "Host": "smtp.sendgrid.net",
    "Port": 587,
    "EnableSsl": true,
    "User": "apikey",
    "From": "workspace.piyush01@gmail.com"
  },
  "ClientSettings": {
    "AllowedClients": [
      "web-ui-v1.0",
      "postman-v1.0",
      "swagger-ui-v1.0"
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "ServiceJwtSettings": {
    "Issuer": "RecoTrackAPI - Service",
    "Audience": "RecoTrackAPI - Email-Service",
    "ExpiryMinutes": 10
  },
  "EmailService": {
    "Url": "https://recotrack-emailservice-men.onrender.com/api/emails/send",
    "OtpUrl": "https://recotrack-emailservice-men.onrender.com/api/emails/send",
    "ServiceToken": "SAP-BTP-Service-Key-Demo"
  },
  "ObjectStorageSettings": {
    "BucketName": "recotrack-dev",
    "Region": "us-east-1",
    "EndpointUrl": "http://localhost:9000",
    "PublicBaseUrl": "http://localhost:9000/recotrack-dev",
    "UsePathStyle": true,
    "UploadUrlExpiryMinutes": 15,
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin"
  },
  "GoogleAuth": {
    "ClientId": "${GOOGLE_CLIENT_ID}"
  }
}
